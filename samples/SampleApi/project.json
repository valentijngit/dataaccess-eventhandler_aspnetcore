{
    "webroot": "wwwroot",
    "version": "1.0.0-*",
    "commands": {
        "web": "Microsoft.AspNetCore.Hosting --server Microsoft.AspNetCore.Server.Kestrel --server.urls http://localhost:51001;https://localhost:44331",
        "ef": "EntityFramework.Commands"
    },
  "dependencies": {
    "AutoMapper": "5.0.0",
    "Microsoft.AspNetCore.Cors": "1.0.0",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Http": "1.0.0",
    "Microsoft.AspNetCore.Http.Extensions": "1.0.0",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Microsoft.AspNetCore.WebUtilities": "1.0.0",
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "Microsoft.Extensions.DependencyInjection": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Microsoft.Extensions.Logging.Console": "1.0.0",
    "Microsoft.Extensions.Logging.Debug": "1.0.0",
    "SeriLog.Extensions.Logging": "1.0.0",
    "SeriLog.Sinks.RollingFile": "2.0.0-beta-465",
    "Swashbuckle.SwaggerGen": "6.0.0-beta901",
    "Swashbuckle.SwaggerUi": "6.0.0-beta901",
    "Digipolis.Common": "2.0.0",
    "Digipolis.DataAccess": "2.0.0",
    "Npgsql.EntityFrameworkCore.PostgreSQL" :  "1.0.0",
    "Digipolis.Errors": "2.0.0",
    "Digipolis.Json": "2.0.0",
    "Digipolis.Web": "2.0.0",
    "Digipolis.EventHandler": "2.0.0",
    "Digipolis.DataAccess.EventHandler": "2.0.0"
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final"
  },
    "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "bower_components",
        ".grunt"
      ]
    }
  },
  
  "publishOptions": {
    "exclude": [
      "node_modules",
      "bower_components",
      "**.xproj",
      "**.user",
      "**.vspscc"
    ]
  }
}
